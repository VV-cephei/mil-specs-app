  "doeApproach": {
    "title": "DOE (Design of Experiments) Approach",
    "description": "Systematic method to optimize wire bonding parameters",
    "methodology": {
      "screening": {
        "purpose": "Identify significant factors",
        "design": "Fractional factorial (2^(k-p))",
        "factors": ["Force", "Power", "Time", "Temperature"],
        "responses": ["Pull strength", "Shear strength", "Ball diameter", "Heel condition"]
      },
      "optimization": {
        "purpose": "Find optimal parameter settings",
        "design": "Response Surface Methodology (RSM) or Central Composite Design (CCD)",
        "goal": "Maximize bond strength while minimizing defects"
      },
      "confirmation": {
        "purpose": "Verify optimal settings",
        "method": "Run confirmation lots at predicted optimum",
        "criteria": "Results within prediction interval"
      }
    },
