  "troubleshootingGuide": {
    "title": "Wire Bonding Troubleshooting Guide",
    "issues": [
      {
        "problem": "Non-Stick on Pad (NSOP)",
        "symptoms": ["Bond doesn't adhere", "Wire pulls off easily", "Impedance monitoring alarm"],
        "possibleCauses": [
          "Pad contamination (organic, oxide)",
          "Insufficient ultrasonic power",
          "Low temperature",
          "Worn capillary"
        ],
        "solutions": [
          "Plasma clean pads before bonding",
          "Increase ultrasonic power (5-10%)",
          "Increase temperature (10-20°C)",
          "Replace capillary",
          "Verify forming gas flow (for Cu)"
        ]
      },
      {
        "problem": "Ball Lift",
        "symptoms": ["Ball separates from pad during pull test", "Clean pad surface after lift"],
        "possibleCauses": [
          "Contamination",
          "Insufficient bonding energy",
          "Pad metallization issues",
          "Oxidation"
        ],
        "solutions": [
          "Improve cleaning process",
          "Increase force, power, or time",
          "Verify pad metallization quality",
          "Check forming gas for Cu wire"
        ]
      },
      {
        "problem": "Cratering",
        "symptoms": ["Silicon visible under lifted pad", "Conchoidal fractures in silicon"],
        "possibleCauses": [
          "Excessive bonding force",
          "Excessive ultrasonic power",
          "Hard wire (Cu)",
          "Thin/weak dielectric"
        ],
        "solutions": [
          "Reduce bonding force",
          "Use ramp-up ultrasonic profile",
          "Increase frequency (120 kHz)",
          "Use softer wire or Pd-Cu",
          "Strengthen pad stack design"
        ]
      },
      {
        "problem": "Heel Cracking",
        "symptoms": ["Crack at stitch heel", "Low pull strength with heel break"],
        "possibleCauses": [
          "Excessive ultrasonic power",
          "Worn capillary edge",
          "Steep loop angle",
          "Work hardening"
        ],
        "solutions": [
          "Reduce ultrasonic power",
          "Replace capillary",
          "Adjust loop profile for gentler angle",
          "Reduce bonding time"
        ]
      },
      {
        "problem": "Golf Club (Ball Misalignment)",
        "symptoms": ["Ball not centered on wire", "Asymmetric ball shape"],
        "possibleCauses": [
          "Capillary misalignment",
          "Worn capillary",
          "EFO spark inconsistency",
          "Wire feed issues"
        ],
        "solutions": [
          "Realign capillary",
          "Replace capillary",
          "Adjust EFO parameters",
          "Check wire tensioner and guides"
        ]
      },
      {
        "problem": "Wire Sweep",
        "symptoms": ["Excessive lateral wire displacement", "Wires touching after mold"],
        "possibleCauses": [
          "Mold compound flow",
          "Insufficient loop stiffness",
          "Long wire spans",
          "Low loop height"
        ],
        "solutions": [
          "Use worked loop (double reverse)",
          "Increase loop height if possible",
          "Optimize mold compound and process",
          "Reduce wire span length"
        ]
      },
      {
        "problem": "Inconsistent Ball Size",
        "symptoms": ["Ball diameter variation", "FAB size inconsistency"],
        "possibleCauses": [
          "EFO current variation",
          "EFO gap inconsistency",
          "Wire feed variation",
          "Forming gas flow variation"
        ],
        "solutions": [
          "Calibrate EFO system",
          "Check and adjust EFO gap",
          "Verify wire tensioner settings",
          "Stabilize forming gas flow"
        ]
      },
      {
        "problem": "Stitch Lift",
        "symptoms": ["Second bond separates from substrate", "Tail not adhered"],
        "possibleCauses": [
          "Substrate contamination",
          "Insufficient bonding parameters",
          "Poor surface finish",
          "Oxidation (Cu wire)"
        ],
        "solutions": [
          "Clean substrate surface",
          "Increase force, power, time for stitch",
          "Verify plating quality",
          "Use security bond (ball on stitch)"
        ]
      }
    ]
  },
