  "cpkRequirements": {
    "title": "Cpk (Process Capability Index) Requirements",
    "definition": "Cpk measures how well a process fits within specification limits",
    "formula": "Cpk = min[(USL - μ)/(3σ), (μ - LSL)/(3σ)]",
    "requirements": {
      "commercial": {
        "minimum": 1.0,
        "target": 1.33,
        "defectRate": "2700 ppm at Cpk=1.0"
      },
      "industrial": {
        "minimum": 1.33,
        "target": 1.5,
        "defectRate": "63 ppm at Cpk=1.33"
      },
      "automotive": {
        "minimum": 1.67,
        "target": 2.0,
        "defectRate": "0.6 ppm at Cpk=1.67"
      },
      "aerospace": {
        "minimum": 1.67,
