{
  "processParameters": {
    "title": "Wire Bonding Process Parameters",
    "description": "Key parameters that control bond quality and must be optimized for each application",
    "primaryParameters": [
      {
        "parameter": "Bonding Force",
        "symbol": "F",
        "unit": "grams-force (gf) or milliNewtons (mN)",
        "description": "Downward force applied by capillary/wedge during bonding",
        "effect": {
          "tooLow": "Weak bond, insufficient deformation, bond lift",
          "tooHigh": "Cratering, pad damage, excessive squash, thin neck"
        },
        "typicalRanges": {
          "ballBond": {
            "finePitch": "15-30 gf",
            "standard": "25-50 gf",
            "power": "40-80 gf"
          },
          "stitchBond": {
            "finePitch": "30-60 gf",
            "standard": "50-100 gf",
            "power": "80-150 gf"
          }
        }
      },
      {
        "parameter": "Ultrasonic Power",
        "symbol": "USG or P",
        "unit": "milliWatts (mW) or arbitrary machine units",
        "description": "Ultrasonic energy applied to scrub oxide and create weld",
        "effect": {
          "tooLow": "Insufficient oxide removal, weak bond, NSOP",
          "tooHigh": "Heel cracking, work hardening, pad damage"
        },
        "typicalRanges": {
          "goldWire": "50-150 mW",
          "copperWire": "80-200 mW (20-30% higher than Au)",
          "aluminumWire": "100-300 mW"
        },
        "frequency": {
          "standard": "60 kHz",
          "highFrequency": "100-140 kHz",
          "note": "Higher frequency allows lower amplitude, reducing damage risk"
        }
      },
      {
        "parameter": "Bonding Time",
        "symbol": "t",
        "unit": "milliseconds (ms)",
        "description": "Duration of ultrasonic energy application",
        "effect": {
          "tooShort": "Incomplete weld formation",
          "tooLong": "Excessive heat generation, reduced throughput"
        },
        "typicalRanges": {
          "ballBond": "8-20 ms",
          "stitchBond": "10-25 ms",
          "heavyWire": "20-50 ms"
        }
      },
      {
        "parameter": "Temperature",
        "symbol": "T",
        "unit": "degrees Celsius (°C)",
        "description": "Substrate/stage temperature during bonding",
        "effect": {
          "tooLow": "Hard wire, difficult bonding, higher force needed",
          "tooHigh": "Intermetallic growth, oxidation, die damage"
        },
        "typicalRanges": {
          "thermosonic": {
            "gold": "150-220°C",
            "copper": "150-200°C",
            "palladiumCopper": "140-180°C"
          },
          "ultrasonic": {
            "aluminum": "Room temperature to 150°C"
          }
        }
      }
    ],
    "secondaryParameters": [
      {
        "parameter": "Impact Velocity",
        "description": "Speed at which capillary contacts the pad",
        "effect": "Higher velocity increases initial deformation but risks cratering"
      },
      {
        "parameter": "Search Speed",
        "description": "Speed of capillary descent during pad detection",
        "typical": "0.1-0.5 mm/s"
      },
      {
        "parameter": "Loop Height",
        "description": "Maximum height of wire loop above die",
        "typical": "75-500 µm depending on package"
      },
      {
        "parameter": "Kink Height",
        "description": "Height for reverse motion during looping",
        "effect": "Controls loop profile shape"
      },
      {
        "parameter": "Reverse Distance",
        "description": "Distance of reverse motion for loop formation",
        "effect": "Controls loop stiffness and profile"
      }
    ]
  },
